Date: Fri, 7 Jan 2022 15:36:05 +0300
Subject: [PATCH] Pull master before applying patch

---
 push-last-commit.sh | 2 ++
 1 file changed, 2 insertions(+)

diff --git a/push-last-commit.sh b/push-last-commit.sh
index 0e05152..0b3a0ba 100755
--- a/push-last-commit.sh
+++ b/push-last-commit.sh
@@ -18,6 +18,8 @@ fi
 
 mv "$PATCH_NAME" $RIVERADMIN_PATH && echo "Moved patch to riveradmin"
 
+git pull origin master;
+
 cd $RIVERADMIN_PATH && git apply "$PATCH_NAME" && echo "The patch have been applied"
 
 rm "$PATCH_NAME" && git add . && git commit -m "$COMMIT_NAME" && echo "Commit"
